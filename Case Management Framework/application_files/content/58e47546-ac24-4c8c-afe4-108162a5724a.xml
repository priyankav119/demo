<?xml version="1.0" encoding="UTF-8"?><contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>CASE_APP_ucMultiFileUploadRenderField</name>
        <uuid>58e47546-ac24-4c8c-afe4-108162a5724a</uuid>
        <description>Used in multifileupload to render a single file upload field</description>
        <parentUuid>_a-0000d9b9-6309-8000-9ba2-011c48011c48_1672</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>=with(&#13;
  local!paddedArray: append(ri!files, null),&#13;
&#13;
    #"SYSTEM_SYSRULES_fileUploadField"(&#13;
      label: if(ri!index = 1, ri!label, ""),&#13;
      target: ri!target,&#13;
      value: local!paddedArray[ri!index],&#13;
      saveInto: {&#13;
        a!save(ri!files, #"96e0d627-3c44-424e-8ecf-64f1c07c933d"(ri!files, ri!index, save!value))&#13;
        &#13;
      },&#13;
      required: if(ri!index = 1, ri!required, false),&#13;
      disabled: ri!disabled&#13;
    )&#13;
&#13;
)</definition>
        <namedTypedValue>
            <name>label</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>files</name>
            <type>
                <name>CollaborationDocument?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>target</name>
            <type>
                <name>CollaborationDocumentOrFolder</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>index</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>disabled</name>
            <type>
                <name>boolean</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>required</name>
            <type>
                <name>boolean</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>interface</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
    <roleMap public="true">

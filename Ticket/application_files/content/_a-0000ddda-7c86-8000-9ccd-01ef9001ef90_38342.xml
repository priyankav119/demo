<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>TA3_GetMonthRange</name>
        <uuid>_a-0000ddda-7c86-8000-9ccd-01ef9001ef90_38342</uuid>
        <description>Displays month-year for a certain interval of months. Pass 1 as the startIndex to return the current month.</description>
        <parentUuid>8e9e4517-6843-4e16-9c62-c0712ebacbe0</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>with(
  local!range: ri!startIndex + enumerate(1 + ri!endIndex - ri!startIndex),
  local!monthDates: apply(
    eomonth(today(), _),
    local!range
  ),
  apply(
    #&quot;_a-0000ddda-7c86-8000-9ccd-01ef9001ef90_38341&quot;,
    local!monthDates
  )
)</definition>
        <namedTypedValue>
            <name>startIndex</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>endIndex</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>

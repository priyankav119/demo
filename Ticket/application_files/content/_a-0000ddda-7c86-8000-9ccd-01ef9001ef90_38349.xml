<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>TA3_DetailsForActivityHistoryEntry</name>
        <uuid>_a-0000ddda-7c86-8000-9ccd-01ef9001ef90_38349</uuid>
        <description>Given an old and new values of a CDT, returns a string message representing the changes</description>
        <parentUuid>fcbcec0c-b5ed-437a-a110-9dbbfa16a4da</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>with(
  local!message: reduce(
    #&quot;_a-0000ddda-7c86-8000-9ccd-01ef9001ef90_38333&quot;(
      _,
      _,
      ri!oldCDT,
      ri!newCDT,
      ri!fields,
      ri!fieldNames,
      ri!fieldTypes
    ),
    &quot;&quot;,
    1 + enumerate(length(ri!fields))
  ),
  if(
    lenb(local!message) &gt; 3950,
    left(local!message, 3950) &amp; &quot;... [Truncated]&quot;,
    local!message
  )
)</definition>
        <namedTypedValue>
            <name>oldCDT</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>newCDT</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>fields</name>
            <type>
                <name>Text?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>fieldNames</name>
            <type>
                <name>Text?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>fieldTypes</name>
            <type>
                <name>Text?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>

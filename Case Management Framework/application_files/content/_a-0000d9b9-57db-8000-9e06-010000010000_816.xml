        <definition>=choose(wherecontains(ri!country, #"_a-0000d9b9-57db-8000-9e06-010000010000_807"), #"_a-0000d9b8-0d4a-8000-9e06-010000010000_782", #"_a-0000d9b5-6c6c-8000-9e06-010000010000_767", #"_a-0000d9b5-6c6c-8000-9e06-010000010000_768", #"_a-0000d9b5-6c6c-8000-9e06-010000010000_766", #"_a-0000d9b5-6c6c-8000-9e06-010000010000_770", #"_a-0000d9b9-57db-8000-9e06-010000010000_806", #"_a-0000d9b8-0d4a-8000-9e06-010000010000_784", #"_a-0000d9b5-6c6c-8000-9e06-010000010000_764", #"_a-0000d9b9-6309-8000-9ba2-011c48011c48_2078")</definition>

        <definition>/*&#13;To Test, load these base test values:&#13;  record: (Point to a Constant of Record Type)&#13;  primaryKeyField:  (If blank, defaults to "id" - if using a Process Backed Record, primaryKeyField must point to a process variable of Integer type)&#13;  searchFields: (The name of the fields you want to search)&#13;&#13;*/&#13;  with(&#13;    local!primaryKeyField: if (isnull(ri!primaryKeyField), "id", ri!primaryKeyField),&#13;    local!labelField: if (isnull(ri!labelField), ri!searchFields[1], ri!labelField),&#13;    #"SYSTEM_SYSRULES_pickerFieldCustom"(&#13;      label: ri!label,&#13;      labelPosition: ri!labelPosition,&#13;      instructions: ri!instructions,&#13;      maxSelections: ri!maxSelections,&#13;      suggestFunction: #"_a-0000d9b9-6309-8000-9ba2-011c48011c48_1458"(&#13;        record: ri!record,&#13;        primaryKeyField: local!primaryKeyField,&#13;        searchFields: ri!searchFields,&#13;        labelField: local!labelField,&#13;        searchTerm: _,&#13;        existingIds: if (ri!requireUniqueValues, #"_a-0000d9b9-6309-8000-9ba2-011c48011c48_1456"( array: ri!selectedValues, index: local!primaryKeyField, default: {0} ), null)&#13;      ),&#13;      selectedLabels: #"_a-0000d9b9-6309-8000-9ba2-011c48011c48_1456"( array: ri!selectedValues, index: local!labelField ),&#13;      value: #"_a-0000d9b9-6309-8000-9ba2-011c48011c48_1456"( array: ri!selectedValues, index: tostring(local!primaryKeyField) ),&#13;      saveInto: {&#13;        ri!saveInto,&#13;        a!save(&#13;          ri!selectedValues, &#13;          { /* This bracket is to force an array result */&#13;            if (or(isnull(save!value), count(save!value) &lt; 1), &#13;              {},&#13;              if (and(count(save!value) = 1, not(isnull(save!value[1])), tointeger(save!value[1]) &gt; 0),&#13;                #"_a-0000d9b9-6309-8000-9ba2-011c48011c48_1457"(&#13;                  record: ri!record,&#13;                  primaryKeyField: local!primaryKeyField,&#13;                  primaryKeyValue: save!value[1]&#13;                ),&#13;                apply(&#13;                  #"_a-0000d9b9-6309-8000-9ba2-011c48011c48_1457"(&#13;                    record: ri!record,&#13;                    primaryKeyField: local!primaryKeyField,&#13;                    primaryKeyValue: _&#13;                  ),&#13;                  save!value&#13;                )&#13;              )&#13;            )&#13;          }&#13;        )&#13;      },&#13;      required: ri!required,&#13;      requiredMessage: ri!requiredMessage,&#13;      readOnly: ri!readOnly,&#13;      disabled: ri!disabled,&#13;      validations: ri!validations,&#13;      validationGroup: ri!validationGroup&#13;    )&#13;  )&#13;</definition>
